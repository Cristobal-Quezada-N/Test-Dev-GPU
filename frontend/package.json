{
  "name": "test-gpu-front",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "lint:fix": "eslint . --fix --fix-type=problem,suggestion,layout --cache",
    "postinstall": "if [ -d .git ]; then git config core.hooksPath frontend/.husky; else echo 'Skipping husky install (no .git)'; fi",
    "prepare": "husky"
  },
  "lint-staged": {
    "frontend/**/*.{ts,tsx,js,vue}":
        ["eslint --fix --fix-type=problem,suggestion,layout"]
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.6",
    "@mdi/font": "7.4.47",
    "@tanstack/vue-query": "^5.85.5",
    "axios": "^1.11.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vuetify": "^3.9.1"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-config-vuetify": "^4.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "npm-run-all2": "^7.0.2",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.41.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.8.0",
    "unplugin-vue-router": "^0.14.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-layouts-next": "^1.0.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.1"
  }
}
